# Example environment variables for hwtest development
# Copy to .env and adjust as needed

# InfluxDB connection (for hwtest-logger)
INFLUXDB_URL=http://localhost:8086
INFLUXDB_ORG=hwtest
INFLUXDB_BUCKET=telemetry
INFLUXDB_TOKEN=hwtest-dev-token

# UUT simulator (for integration tests)
UUT_URL=http://192.168.68.xxx:8080

# CAN interface (for integration tests)
CAN_INTERFACE=can0
